import pygame

pygame.init()

width, height = 800, 800
window = pygame.display.set_mode((width, height))
window.fill("white")

run = True
while run is True:
    for events in pygame.event.get():
        if events.type == pygame.QUIT:
            run = False
            
    pygame.display.update()
    pygame.display.flip()
    
pygame.quit()
